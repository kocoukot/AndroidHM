<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/videoNameNew"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="New video name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/videoURLDownload"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="URL"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/videoNameNew" />

    <Button
        android:id="@+id/downLoadVideo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/auth_login"
        app:layout_constraintTop_toBottomOf="@id/videoURLDownload" />


</androidx.constraintlayout.widget.ConstraintLayout>